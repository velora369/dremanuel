  <rect x="440" y="660" width="60" height="30" fill="#FFFFFF" stroke="#DDDDDD" stroke-width="1"/>
  <circle cx="420" cy="730" r="15" fill="#333333"/>
  <circle cx="480" cy="730" r="15" fill="#333333"/>
  <rect x="445" y="680" width="30" height="20" fill="#F44336"/>
  <rect x="450" y="670" width="5" height="15" fill="#FFFFFF"/>
  <rect x="445" y="675" width="15" height="5" fill="#FFFFFF"/>
  
