    <rect x="780" y="250" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    <rect x="850" y="250" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    
    <rect x="710" y="350" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    <rect x="780" y="350" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    <rect x="850" y="350" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    
    <rect x="710" y="450" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    <rect x="780" y="450" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
