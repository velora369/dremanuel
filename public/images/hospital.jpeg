  <circle cx="180" cy="600" r="40" fill="#388E3C"/>
  <circle cx="1020" cy="600" r="40" fill="#388E3C"/>
  <circle cx="120" cy="580" r="30" fill="#388E3C"/>
  <circle cx="1080" cy="580" r="30" fill="#388E3C"/>
  
