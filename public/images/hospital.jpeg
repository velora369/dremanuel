  <!-- Center entrance -->
  <rect x="525" y="500" width="150" height="150" fill="#FFFFFF" stroke="#DDDDDD" stroke-width="2"/>
  <rect x="565" y="550" width="70" height="100" fill="#1995AD" stroke="#DDDDDD" stroke-width="1"/>
  
  <!-- Roof -->
  <rect x="240" y="190" width="720" height="20" fill="#1995AD" stroke="#DDDDDD" stroke-width="1"/>
  
  <!-- Hospital Sign -->
  <rect x="525" y="150" width="150" height="40" fill="#1995AD" stroke="#DDDDDD" stroke-width="1"/>
  <text x="600" y="178" text-anchor="middle" font-family="Arial" font-size="24" fill="white">HOSPITAL</text>
  
  <!-- Red cross symbol -->
  <rect x="585" y="210" width="30" height="80" fill="#E53935" rx="5" ry="5"/>
  <rect x="560" y="235" width="80" height="30" fill="#E53935" rx="5" ry="5"/>
  
