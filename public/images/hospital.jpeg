    <rect x="440" y="350" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    
    <rect x="300" y="450" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
    <rect x="370" y="450" width="50" height="70" fill="#A1D6E2" stroke="#DDDDDD" stroke-width="1"/>
